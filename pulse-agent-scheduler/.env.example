# RPC URL for Solana network
# Use devnet for testing, mainnet-beta for production
RPC_URL=https://api.devnet.solana.com

# Scheduler Authority Keypair (base64 encoded)
# Generate with: solana-keypair new
# Then encode with: cat <keypair_file> | base64
SCHEDULER_KEYPAIR=

# Agent Owner (wallet that owns the agent)
AGENT_OWNER=

# Agent ID to purchase tickets for
AGENT_ID=my-agent

# Event Organizer (event creator wallet)
EVENT_ORGANIZER=

# Event ID
EVENT_ID=event-1

# Ticket Tier ID to purchase
TIER_ID=VIP
